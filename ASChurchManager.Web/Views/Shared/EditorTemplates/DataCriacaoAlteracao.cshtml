@*Exemplo de utilização: @Html.EditorFor(model => model, "DataCriacaoAlteracao")*@

@model ASChurchManager.Web.ViewModels.Shared.EntityViewModelBase

<p style="clear: both" />
<div>
    <hr />
    <div class="form-group">
        <div class="col-sm-3">
            @Html.LabelFor(model => model.DataCriacao, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => model.DataCriacao, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        </div>
        <div class="col-sm-3">
            @Html.LabelFor(model => model.DataAlteracao, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => model.DataAlteracao, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        </div>
    </div>
</div>
<p style="clear: both" />